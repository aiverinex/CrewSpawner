# Meta-Crew Spawner Environment Variables
# Copy this file to .env and fill in your API keys

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Configuration  
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Groq Configuration
GROQ_API_KEY=your_groq_api_key_here

# Mistral Configuration
MISTRAL_API_KEY=your_mistral_api_key_here

# Application Configuration
SECRET_KEY=your_secret_key_for_sessions_change_in_production
DEBUG=False
PORT=5000

# Optional: Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=meta_crew_spawner.log

# Optional: Rate Limiting
MAX_REQUESTS_PER_MINUTE=60
MAX_CONCURRENT_CREWS=5

# Optional: Cache Configuration
ENABLE_CACHE=True
CACHE_TTL=3600

# Security Settings
ALLOWED_ORIGINS=http://localhost:5000,https://yourdomain.com
